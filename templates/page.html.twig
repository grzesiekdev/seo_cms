{% extends "base.html.twig" %}
{% block title %}
    {{ page.getTitle }}
{% endblock %}
{% block description %}
    {{ page.description }}
{% endblock %}
{% block body %}
    <div>
        <h1 class="p-3">{{ page.name }}</h1>
        <div class="m-3">
            {{ page.content|raw }}
        </div>

    </div>

{% endblock %}